{
	"name": "abi-website",
	"version": "0.9.0",
	"private": true,
	"scripts": {
		"install": "cd frontend && npm install && cd ../backend && npm install",
		"production": "NODE_ENV=production sh -c 'cd frontend && npm run build && cd ../backend && npm run start'",
		"dev": "cd backend && npm run dev",
		"mail": "cd backend && npm run mail",
		"query": "cd backend && npm run query",
		"serve": "cd frontend && npm run serve",
		"build": "cd frontend && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aarondiel/abi-website.git"
	},
	"keywords": [
		"vue",
		"vuejs",
		"express",
		"mongodb",
		"abi",
		"website"
	],
